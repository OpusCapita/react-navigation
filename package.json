{
  "name": "@opuscapita/react-navigation",
  "version": "0.1.1",
  "description": "Start write new project with no effort.",
  "scripts": {
    "npm-build": "rimraf lib && webpack --config webpack.production.config.js",
    "link-mode": "webpack --watch --config ./webpack.link.config.js",
    "lint": "eslint src/client",
    "publish-release": "npm run npm-build && npm-publish --release",
    "prepublish": "npm run npm-build",
    "start": "showroom-scan src && node www/index",
    "gh-pages-deploy": "showroom-scan src && sh ./gh-pages-update.sh",
    "test": "mocha --compilers js:babel-core/register --require ignore-styles --require ./test/setup.js --recursive src/**/*.spec.js"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
